pipeline{
    agent any
    stages{
        stage("clone"){
            steps{
               bat "git clone https://github.com/sbtalk71/jenkins-demo2.git"
            }
        }
        stage("compile"){
            steps{
            bat "mvn clean compile"
            }
        }
        
         stage("package"){
            steps{
            bat "mvn package"
            }
        }
       
        }
   
}